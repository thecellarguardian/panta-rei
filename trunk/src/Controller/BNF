
command
	create-statement
	set-statement
	:simulate:
	:help:
	:quit:

create-statement
	:create: object-statement
	
object-statement
	:periodic-task: int_(a) int_(C) int_(D) int_(T)
	
set-statement
	:set: property-statement
		
property-statement
	:timer-length: int_(length)
	:scheduler: scheduling-algorithm
	
scheduling-algorithm
	:RM:
	:DM:
	:EDF:
